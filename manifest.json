{
    "name": "kwitty",
    "description": "A chrome application of twitter",
    "version": "1.4.5",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "options_page": "options.html",
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "permissions": [
        "unlimitedStorage",
        "notifications",
        "tabs", 
        "*://*.twitter.com/*",
        "*://*.appspot.com/*"
    ]
}

